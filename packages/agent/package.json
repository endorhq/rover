{
  "name": "@endorhq/agent",
  "private": true,
  "version": "0.0.0-dev",
  "description": "Provide a library to define and manage AI agent workflows for Rover",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "bin": {
    "rover-agent": "dist/cli.mjs"
  },
  "files": ["LICENSE.md", "dist/*"],
  "scripts": {
    "build": "tsdown --env.TSUP_DEV=false",
    "build-dev": "tsdown --env.TSUP_DEV=true",
    "dev": "TSUP_DEV=true tsdown --env.TSUP_DEV=true --watch src/",
    "start": "node --enable-source-maps dist/cli.mjs",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "author": "The Endor team",
  "dependencies": {
    "yaml": "^2.8.1",
    "ansi-colors": "catalog:runtime",
    "commander": "catalog:runtime",
    "execa": "catalog:runtime",
    "ora": "^8.2.0",
    "zod": "catalog:runtime"
  },
  "devDependencies": {
    "rover-core": "workspace:*",
    "rover-schemas": "workspace:*",
    "@types/node": "catalog:build",
    "@vitest/ui": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:build"
  }
}
