{
  "name": "rover",
  "version": "1.5.1",
  "description": "WIP",
  "main": "index.js",
  "type": "module",
  "bin": {
    "rover": "dist/index.js"
  },
  "files": [
    "README.md",
    "dist/*"
  ],
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "TSUP_DEV=true tsdown --watch src/ src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "vitest --run --silent",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [],
  "author": "The Endor team",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "commander": "^14.0.0",
    "enquirer": "^2.4.1",
    "execa": "^9.6.0",
    "nanoid": "^5.0.9",
    "ora": "^8.2.0",
    "yocto-spinner": "^1.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/tmp": "^0.2.6",
    "@vitest/ui": "^2.1.8",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  }
}
