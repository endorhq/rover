{
  "name": "rover-telemetry",
  "version": "1.0.0",
  "private": true,
  "description": "Provide basic telemetry to the rover CLI and extension",
  "main": "dist/index.mjs",
  "types": "src/index.ts",
  "type": "module",
  "files": ["LICENSE.md", "dist/*"],
  "scripts": {
    "build": "tsdown --config-loader unrun --env.TSUP_DEV=false",
    "build-dev": "TSUP_DEV=true tsdown --config-loader unrun --env.TSUP_DEV=true",
    "dev": "TSUP_DEV=true tsdown --env.TSUP_DEV=true --watch src/",
    "start": "echo 'Telemetry package is not a CLI'",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit"
  },
  "author": "The Endor team",
  "dependencies": {
    "posthog-node": "^5.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "catalog:build",
    "@types/react": "^19.1.8",
    "@types/uuid": "^10.0.0",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
